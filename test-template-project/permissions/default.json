{
  "$schema": "../../../schemas/permissions.json",
  "version": "1.0.0",
  "permissions": {
    "filesystem": {
      "read": true,
      "write": true,
      "paths": [
        "./src/**",
        "./public/**",
        "./tests/**",
        "./docs/**",
        "./package.json",
        "./tsconfig.json",
        "./vite.config.*",
        "./webpack.config.*"
      ]
    },
    "network": {
      "outbound": true,
      "domains": [
        "npmjs.com",
        "github.com",
        "stackoverflow.com",
        "developer.mozilla.org"
      ]
    },
    "commands": {
      "allowed": [
        "npm",
        "yarn",
        "pnpm",
        "node",
        "npx",
        "git",
        "eslint",
        "prettier",
        "jest",
        "cypress"
      ]
    }
  }
}